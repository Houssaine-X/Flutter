# Google Cloud API Key (used for Gemini AI and Text-to-Speech)
# Get your API key from: https://console.cloud.google.com/apis/credentials
GOOGLE_CLOUD_API_KEY=your_google_cloud_api_key_here

# Firebase API Keys
# Get these from your Firebase Console: https://console.firebase.google.com
FIREBASE_WEB_API_KEY=your_firebase_web_api_key
FIREBASE_ANDROID_API_KEY=your_firebase_android_api_key
FIREBASE_IOS_API_KEY=your_firebase_ios_api_key

# Firebase Project Configuration
FIREBASE_PROJECT_ID=your_project_id
FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
FIREBASE_STORAGE_BUCKET=your_storage_bucket
FIREBASE_AUTH_DOMAIN=your_auth_domain

# Firebase App IDs
FIREBASE_WEB_APP_ID=your_web_app_id
FIREBASE_ANDROID_APP_ID=your_android_app_id
FIREBASE_IOS_APP_ID=your_ios_app_id
FIREBASE_WINDOWS_APP_ID=your_windows_app_id

# iOS Client ID
FIREBASE_IOS_CLIENT_ID=your_ios_client_id

